<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.JDERP.ERP.mapper.EmployeeMapper">

    <select id="selectEmployeeList" resultType="com.JDERP.ERP.model.Employee">
        SELECT emp_cd AS empCd,
               emp_nm AS empNm,
               dept_cd AS deptCd,
               yn_out AS ynOut,
               dept_position AS deptPosition,
               tp_weekly AS tpWeekly
          FROM erpdb.emp
    </select>

</mapper>